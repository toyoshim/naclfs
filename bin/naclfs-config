#!/bin/sh

function _wrap() {
  /bin/echo -n "-Xlinker --wrap -Xlinker $1 "
}

function libs() {
  # _wrap "fcntl"
  # _wrap "mkdir"
  # _wrap "opendir"
  # _wrap "rewinddir"
  # _wrap "readdir"
  # _wrap "closedir"
  # _wrap "chdir"
  echo
}

function objs() {
  /bin/echo -n "$1/src/wrap.o $1/src/naclfs.o $1/src/filesystem.o "
  /bin/echo -n "$1/src/port_filesystem.o $1/src/html5_filesystem.o "
  echo
}

case $1 in
  --libs) libs;;
  --objs) objs $2;;
esac

